<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MunchBot"
        tools:targetApi="31">
        <activity
            android:name=".ui.fragments.signUp.SignUp1Fragments"
            android:exported="false" />
        <activity
            android:name=".Sign_Up"
            android:exported="false" />
        <activity
            android:name=".ui.fragments.signUp.SignUp1Fragment"
            android:exported="false"
            android:theme="@style/Theme.MunchBot" />
        <activity
            android:name=".ui.auth.SignUp2"
            android:exported="false" />
        <activity
            android:name=".ui.Splash.Splash"
            android:exported="true"
            android:theme="@style/Theme.MunchBot.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GetStarted"
            android:exported="true" /> <!-- <activity android:name="..GetStarted1Fragment" android:exported="true"/> -->
        <!-- <activity android:name=".GetStarted2Fragment" android:exported="true"/> -->
        <!-- <activity android:name=".GetStarted3Fragment" android:exported="true"/> -->
        <activity
            android:name=".ui.auth.Login"
            android:exported="true" />
        <activity
            android:name=".ui.auth.SignUp"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>